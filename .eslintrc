{
    "parser": "babel-eslint",
    "extends": "standard",
    "env": {
        "browser": true
    },
    "rules": {
        "quotes": [
            2,
            "single"
        ],
        "indent": [2, 2],
        "comma-dangle": 0,
        "no-mixed-requires": [0],
        "no-underscore-dangle": [0],
        "no-else-return": 0
    }
}